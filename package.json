{
  "name": "angular-modules-template",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "20.3.14",
    "@angular/compiler": "20.3.14",
    "@angular/core": "20.3.14",
    "@angular/forms": "20.3.14",
    "@angular/platform-browser": "20.3.14",
    "@angular/router": "20.3.14",
    "rxjs": "7.8.0",
    "tslib": "2.3.0",
    "zone.js": "0.15.0"
  },
  "devDependencies": {
    "@angular/build": "20.3.12",
    "@angular/cli": "20.3.12",
    "@angular/compiler-cli": "20.3.14",
    "@types/jest": "30.0.0",
    "@types/node": "24.10.1",
    "jest": "30.2.0",
    "jest-environment-jsdom": "30.2.0",
    "jest-html-reporters": "3.1.7",
    "jest-preset-angular": "15.0.3",
    "ts-jest": "29.4.5",
    "typescript": "5.9.2"
  }
}
